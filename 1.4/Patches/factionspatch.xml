<?xml version="1.0" encoding="utf-8"?>

<!-- adding new xenos to existing factions -->
<Patch>
	
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/FactionDef[@Name="OutlanderFactionBase" or @Name="TribeBase" or @Name="AncientsBase" or @Name="PirateBandBase"]/xenotypeSet/xenotypeChances</xpath>
		<value>
			<elf_wood>0.05</elf_wood>
            <elf_dark>0.05</elf_dark>
            <dwarf_hill>0.05</dwarf_hill>
            <dwarf_mountain>0.05</dwarf_mountain>
            <orc_brutal>0.05</orc_brutal>
            <warforged>0.05</warforged>
            <tabaxi>0.05</tabaxi>           
            <loxodon>0.05</loxodon>
            <shifter>0.05</shifter>
            <dragonborn>0.05</dragonborn>
            <troll>0.05</troll>
            <halfling>0.05</halfling>
            <goblin>0.05</goblin>
            <undead>0.05</undead>
		</value>
	</Operation>  
		
			
</Patch>
