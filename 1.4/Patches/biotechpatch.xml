<?xml version="1.0" encoding="utf-8"?>

<!-- making it possible for new xenos to show up as vampires -->
<Patch>
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/XenotypeDef[defName="Sanguophage"]/doubleXenotypeChances</xpath>
		<value>
			<elf_wood>0.2</elf_wood>
            <elf_dark>0.2</elf_dark>
            <dwarf_hill>0.2</dwarf_hill>
            <dwarf_mountain>0.2</dwarf_mountain>
            <orc_brutal>0.2</orc_brutal>            
            <tabaxi>0.2</tabaxi>           
            <loxodon>0.2</loxodon>
            <shifter>0.2</shifter>
            <dragonborn>0.2</dragonborn>            
            <halfling>0.2</halfling>
            <goblin>0.2</goblin>           
		</value>
	</Operation>
    
    
    
    
</Patch>
