<?xml version="1.0" encoding="utf-8"?>

<!-- making it possible for new xenos to show up as vampires -->
<Patch>
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/XenotypeDef[defName="Sanguophage"]/doubleXenotypeChances</xpath>
		<value>
			<elf_wood>0.05</elf_wood>
            <elf_dark>0.05</elf_dark>
            <dwarf_hill>0.05</dwarf_hill>
            <dwarf_mountain>0.05</dwarf_mountain>
            <orc_brutal>0.05</orc_brutal>            
            <tabaxi>0.05</tabaxi>           
            <loxodon>0.05</loxodon>
            <shifter>0.05</shifter>
            <dragonborn>0.05</dragonborn>            
            <halfling>0.05</halfling>
            <goblin>0.05</goblin>           
		</value>
	</Operation>
    
    
    
    
</Patch>
