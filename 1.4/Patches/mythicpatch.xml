<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Biotech Expansion - Mythic</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Normal</success>
			<operations>	
				
<!-- remove Biotech Expansion - Mythic version of xenotypes (have reworked in new defs) -->
				<li Class="PatchOperationRemove">
					<xpath>Defs/XenotypeDef[defName="Glintscale"]</xpath>
				</li>				
				<li Class="PatchOperationRemove">
					<xpath>Defs/XenotypeDef[defName="Ashfeather"]</xpath>
				</li>			
				
<!-- remove Biotech Expansion - Mythic faction (intend to rework later using new versions of xenos) -->
				<li Class="PatchOperationRemove">
					<xpath>Defs/FactionDef[defName="TribeMythicHouseFire"]</xpath>
				</li>
			</operations>
   		 </match>
	</Operation>
</Patch>
